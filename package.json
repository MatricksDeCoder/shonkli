    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "helmet": "^4.2.0",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.1",
    "mongoose": "^5.10.13",
