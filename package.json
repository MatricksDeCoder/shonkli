{
  "name": "shonkli",
  "version": "1.0.0",
  "description": "a simple url shortener",
  "main": "index.js",
  "engines": {
    "node": "14"
},
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node index.js",
    "server": "nodemon index.js",
    "client": "npm run start --prefix client",
